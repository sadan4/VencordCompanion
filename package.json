{
    "name": "vencord-user-companion",
    "displayName": "Vencord Companion Extended",
    "version": "1.2.0",
    "description": "Vencord Companion is a vscode extension to test Vencord patches & webpack finds right from the comfort of your IDE",
    "categories": [
        "Other",
        "Snippets",
        "Testing"
    ],
    "keywords": [
        "Discord",
        "Vencord",
        "Modding",
        "Typescript"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sadan4/VencordCompanion"
    },
    "license": "GPL-3.0",
    "author": {
        "name": "sadan",
        "url": "https://sadan.zip"
    },
    "contributors": [
        {
            "name": "Vendicated",
            "email": "vendicated+npm@riseup.net"
        }
    ],
    "publisher": "sadan",
    "main": "./dist/extension.js",
    "scripts": {
        "build": "node ./scripts/build.mts",
        "watch": "node ./scripts/watch.mts",
        "eslint": "eslint --cache",
        "lint": "pnpm eslint && pnpm typecheck",
        "prepackage": "pnpm build && pnpm lint",
        "package": "vsce pack",
        "publishExtension": "pnpm build && pnpm lint && vsce publish --no-dependencies",
        "publishOvsx": "pnpm build && pnpm lint && ovsx publish --no-dependencies",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "genTypes": "node scripts/generateSettings/index.mts",
        "postinstall": "pnpm genTypes"
    },
    "dependencies": {
        "@intrnl/xxhash64": "^0.1.2",
        "@sadan4/devtools-pretty-printer": "catalog:",
        "@vencord-companion/ast-parser": "workspace:",
        "@vencord-companion/shared": "workspace:",
        "@vencord-companion/vencord-ast-parser": "workspace:",
        "@vencord-companion/webpack-ast-parser": "workspace:",
        "fast-diff": "^1.3.0",
        "nanoid": "^5.0.9",
        "typescript": "catalog:",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^3.0.1",
        "@stylistic/eslint-plugin-ts": "^4.2.0",
        "@types/node": "^24.5.0",
        "@types/vscode": "1.80.0",
        "@types/ws": "^8.5.14",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@typescript-eslint/parser": "^8.23.0",
        "@vencord-companion/tsconfig": "workspace:",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "@vscode/vsce": "^3.6.0",
        "esbuild": "^0.25.0",
        "eslint": "^9.19.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "glob": "^11.0.1",
        "jiti": "^2.4.2",
        "ovsx": "^0.10.5",
        "resolve-tspaths": "^0.8.23",
        "source-map-support": "^0.5.21",
        "typescript-eslint": "^8.23.0",
        "vitest": "catalog:"
    },
    "contributes": {
        "configuration": {
            "title": "Vencord Companion",
            "properties": {
                "vencord-user-companion.showSidebar": {
                    "default": false,
                    "description": "Show the sidebar. This is intened to be used by workspaces in their .vscode/settings.json file, use alwaysShowSidebar instead to override this setting",
                    "type": "boolean"
                },
                "vencord-user-companion.alwaysShowSidebar": {
                    "default": false,
                    "description": "Always show the sidebar",
                    "type": "boolean"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vencord-companion-panel",
                    "title": "Companion",
                    "icon": "assets/oneko.svg"
                }
            ]
        },
        "views": {
            "vencord-companion-panel": [
                {
                    "id": "vencordSettings",
                    "name": "Settings",
                    "when": "config.vencord-companion.showSidebar || config.vencord-companion.alwaysShowSidebar",
                    "visibility": "visible",
                    "icon": "assets/oneko.svg"
                }
            ]
        },
        "commands": [
            {
                "command": "vencord-companion.diffModuleSearch",
                "title": "Diff Module Search"
            },
            {
                "command": "vencord-companion.diffModule",
                "title": "Diff Module"
            },
            {
                "command": "vencord-companion.extract",
                "title": "Extract"
            },
            {
                "command": "vencord-companion.extractSearch",
                "title": "Extract With Search"
            }
        ],
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/ts.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/ts.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/tsx.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript",
        "onLanguage:typescriptreact"
    ],
    "engines": {
        "vscode": "^1.80.0"
    },
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#ed80a7",
        "theme": "dark"
    },
    "extensionKind": [
        "ui"
    ],
    "homepage": "https://github.com/sadan4/VencordCompanion",
    "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
    "pnpm": {
        "patchedDependencies": {
            "@types/ws@8.5.14": "patches/@types__ws@8.5.14.patch"
        },
        "onlyBuiltDependencies": [
            "@vscode/vsce-sign",
            "esbuild",
            "keytar"
        ]
    },
    "_vsce": "see https://github.com/microsoft/vscode-vsce/blob/main/src/main.ts for types",
    "vsce": {
        "dependencies": false,
        "githubBranch": "main"
    }
}
