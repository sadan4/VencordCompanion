{
    "name": "vencord-user-companion",
    "displayName": "Vencord Companion Extended",
    "version": "1.5.1",
    "description": "Vencord Companion is a vscode extension to test Vencord patches & webpack finds right from the comfort of your IDE",
    "categories": [
        "Other",
        "Snippets",
        "Testing"
    ],
    "keywords": [
        "Discord",
        "Vencord",
        "Modding",
        "Typescript"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sadan4/VencordCompanion"
    },
    "license": "GPL-3.0",
    "author": {
        "name": "sadan",
        "url": "https://sadan.zip"
    },
    "contributors": [
        {
            "name": "Vendicated",
            "email": "vendicated+npm@riseup.net"
        }
    ],
    "publisher": "sadan",
    "main": "./dist/extension.js",
    "scripts": {
        "buildDeps": "pnpm -r --parallel -F \"./parsers/packages/*\" build",
        "prebuild": "pnpm buildDeps",
        "build": "node ./scripts/build.mts",
        "eslint": "eslint --cache",
        "eslint:vscode:files": "pnpm eslint | grep -P ^[\\\\w/].*\\\\.[mc]?[jt]sx?$",
        "eslint:vscode": "bash -c 'code $(pnpm --silent eslint:vscode:files)'",
        "lint": "conc pnpm:eslint pnpm:typecheck",
        "prepackage": "conc pnpm:build pnpm:lint",
        "package": "vsce pack",
        "publishExtension": "pnpm prepackage && vsce publish --no-dependencies",
        "publishOvsx": "pnpm prepackage && ovsx publish --no-dependencies",
        "typecheck": "tsc --noEmit -p ./tsconfig.ext.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui --api 9972",
        "genTypes": "node scripts/generateSettings/index.mts",
        "postinstall": "conc pnpm:genTypes pnpm:buildDeps"
    },
    "dependencies": {
        "@intrnl/xxhash64": "^0.1.2",
        "@sadan4/devtools-pretty-printer": "^1.0.4",
        "@vencord-companion/ast-parser": "link:./parsers/packages/ast-parser",
        "@vencord-companion/shared": "link:./parsers/packages/shared",
        "@vencord-companion/vencord-ast-parser": "link:./parsers/packages/vencord-ast-parser",
        "@vencord-companion/webpack-ast-parser": "link:./parsers/packages/webpack-ast-parser",
        "fast-diff": "^1.3.0",
        "nanoid": "^5.0.9",
        "typescript": "catalog:",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^3.0.1",
        "@stylistic/eslint-plugin-ts": "^4.2.0",
        "@types/node": "catalog:",
        "@types/vscode": "1.80.0",
        "@types/ws": "^8.5.14",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@typescript-eslint/parser": "^8.23.0",
        "@vitest/coverage-v8": "catalog:",
        "@vitest/ui": "catalog:",
        "@vscode/vsce": "^3.6.0",
        "concurrently": "^9.2.1",
        "esbuild": "^0.25.0",
        "eslint": "^9.19.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^61.0.2",
        "eslint-plugin-unused-imports": "^4.1.4",
        "glob": "^11.0.1",
        "jiti": "^2.4.2",
        "ovsx": "^0.10.5",
        "resolve-tspaths": "^0.8.23",
        "source-map-support": "^0.5.21",
        "typescript-eslint": "^8.23.0",
        "vitest": "catalog:"
    },
    "contributes": {
        "configuration": {
            "title": "Vencord Companion",
            "properties": {
                "vencord-user-companion.showSidebar": {
                    "default": false,
                    "description": "Show the sidebar. This is intened to be used by workspaces in their .vscode/settings.json file, use alwaysShowSidebar instead to override this setting",
                    "type": "boolean"
                },
                "vencord-user-companion.alwaysShowSidebar": {
                    "default": false,
                    "description": "Always show the sidebar",
                    "type": "boolean"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vencord-companion-panel",
                    "title": "Companion",
                    "icon": "assets/oneko.svg"
                }
            ]
        },
        "views": {
            "vencord-companion-panel": [
                {
                    "id": "vencordSettings",
                    "name": "Settings",
                    "when": "config.vencord-companion.showSidebar || config.vencord-companion.alwaysShowSidebar || config.vencord-user-companion.showSidebar || config.vencord-user-companion.alwaysShowSidebar",
                    "visibility": "visible",
                    "icon": "assets/oneko.svg"
                }
            ]
        },
        "commands": [
            {
                "command": "vencord-companion.diffModuleSearch",
                "title": "Diff Module Search"
            },
            {
                "command": "vencord-companion.diffModule",
                "title": "Diff Module"
            },
            {
                "command": "vencord-companion.extract",
                "title": "Extract"
            },
            {
                "command": "vencord-companion.extractSearch",
                "title": "Extract With Search"
            }
        ],
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/ts.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/ts.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/tsx.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript",
        "onLanguage:typescriptreact"
    ],
    "engines": {
        "vscode": "^1.80.0"
    },
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#ed80a7",
        "theme": "dark"
    },
    "extensionKind": [
        "ui"
    ],
    "homepage": "https://github.com/sadan4/VencordCompanion",
    "pnpm": {
        "onlyBuiltDependencies": [
            "@vscode/vsce-sign",
            "esbuild",
            "keytar"
        ]
    },
    "_vsce": "see https://github.com/microsoft/vscode-vsce/blob/main/src/main.ts for types",
    "vsce": {
        "dependencies": false,
        "githubBranch": "main"
    },
    "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
